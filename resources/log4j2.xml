<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<File name="FileAppender" fileName="logs/AnalyzeChangeSet.log" append="false">
			<PatternLayout>
				<Pattern>%d %p %c [%t] %m%n</Pattern>
			</PatternLayout>
		</File>
	</Appenders>
	<Loggers>
		<Root level="ALL">
			<AppenderRef ref="FileAppender" level="INFO" />
		</Root>
	</Loggers>
</Configuration>